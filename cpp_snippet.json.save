		  "template<typename T>",
		  "T power(T x, T y){",
		  "    if (!y)",
		  "        return 1;",
		  "    if (y == 1)",
		  "        return x;",
		  "    if (y & 1)",
		  "        return power(x, y/2) * power(x, y/2) * x;",
		  "    else",
		  "    {",
		  "        return power(x, y/2) * power(x, y/2);",
		  "    }   ",
		  "}"
		],
