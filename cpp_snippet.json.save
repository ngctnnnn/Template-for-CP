		  "//Binary Search",
		  "template<typename T>",
		  "T bs(T a[], T l, T r, T x){",
		  "    T mid = (l + r)/2;",
		  "    if (r >= l){",
		  "        if (a[mid] == x)",
		  "            return mid;",
		  "        if (a[mid] > x)",
		  "            return (a, l, mid - 1, x);",
		  "        return (a, mid + 1, r, x);",
		  "    }",
		  "    return -1;",
		  "}"
		],
		"description": "binary_search"
