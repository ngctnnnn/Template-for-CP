		  "//factorial",
		  "template<typename T>",
		  "T giaithua(T n){",
		  "    T dp[n + 1];",
		  "    dp[1] = 1;",
		  "    dp[2] = 2;",
		  "    loopeach(i, 3, n + 1)",
		  "        dp[i] = dp[i - 1] * dp[i - 2];",
		  "    return dp[n + 1];",
		  "}"
